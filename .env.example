# Database Configuration
DATABASE_URL=postgresql://vibecaas:password@localhost:5432/vibecaas
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# API Configuration
PORT=8000
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# NVIDIA Cloud Configuration
NVIDIA_CLOUD_API_KEY=your-nvidia-cloud-api-key
NVIDIA_CLOUD_PROJECT_ID=your-project-id
NVIDIA_CLOUD_REGION=us-west-1

# Container Registry
CONTAINER_REGISTRY=your-registry.com
REGISTRY_USERNAME=your-username
REGISTRY_PASSWORD=your-password

# Kubernetes Configuration
KUBECONFIG_PATH=~/.kube/config
CLUSTER_NAME=vibecaas-cluster

# Monitoring
LOG_LEVEL=info
METRICS_ENABLED=true

# Storage
STORAGE_BUCKET=vibecaas-storage
STORAGE_REGION=us-west-1